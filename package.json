{
	"name": "javascript-4-assignment-1-duckshop-frontend",
	"version": "0.0.0",
	"author": "Henrik Berglund",
	"license": "MIT",
	"description": "SvelteKit template with Typescript, Vitest and UnoCSS",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:playwright": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "npx gh-pages -d build --dotfiles",
		"release": "commit-and-tag-version",
		"postrelease": "git push --follow-tags origin main"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.1.21",
		"@julr/unocss-preset-forms": "^0.0.5",
		"@julr/unocss-preset-heropatterns": "^1.0.2",
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.3",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/svelte": "^4.0.5",
		"@types/dom-view-transitions": "^1.0.4",
		"@types/jest": "^29.5.8",
		"@unocss/svelte-scoped": "^0.57.2",
		"@vitest/ui": "^0.34.6",
		"commit-and-tag-version": "^12.0.0",
		"hvb-console": "^0.0.6",
		"jsdom": "^22.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unocss": "^0.57.2",
		"unocss-preset-theme": "^0.11.0",
		"vite": "^4.5.0",
		"vitest": "^0.32.4"
	},
	"type": "module"
}
